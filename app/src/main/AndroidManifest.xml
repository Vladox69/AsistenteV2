<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pruebas">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/icono_app"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pruebas">
        <activity android:name=".productosLista"></activity>
        <activity android:name=".listasUsuario" />
        <activity android:name=".crearLista" />
        <activity android:name=".nuevaLista" />
        <activity android:name=".Productos" />
        <activity android:name=".Principal" />
        <activity android:name=".RegistroUsuario" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>